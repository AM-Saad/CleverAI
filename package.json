{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev --port 5173",
    "generate": "nuxt generate",
    "preview": "yarn prebuild && yarn build:inject && nuxt preview",
  
    "build": "nuxt build --vercel-preset",
    "sw:build": "esbuild sw-src/index.ts --bundle --format=iife --outfile=public/sw.js --platform=browser --target=es2019",
    "sw:check": "node scripts/check-sw-placeholder.cjs",
    "prebuild": "yarn sw:build && node scripts/check-sw-placeholder.cjs",
    "test:pwa-offline": "playwright test tests/pwa-offline-basic.spec.ts --project=chromium",
    "build:inject": "yarn sw:build && yarn sw:check && nuxt build --vercel-preset",
    "postinstall": "nuxt prepare",
    "start": "nuxt start",
   
    "sitemap": "vue-cli-service sitemap",
    
    "db:generate": "prisma generate --schema=server/prisma/schema.prisma",
    "db:sync": "prisma generate --schema=server/prisma/schema.prisma && prisma db push --schema=server/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=server/prisma/schema.prisma",
    "db:seed": "tsx server/prisma/seed.ts",
    "notifications:cleanup": "tsx server/utils/cleanupSubscriptions.ts",
    
    "prepare": "husky install",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . --check",
    "lintfix": "yarn lint:eslint --fix && yarn lint:prettier --write",
    "format": "prettier --write \"app/{components,pages,plugins,middleware,layouts,composables,assets}/**/*.{js,jsx,ts,tsx,vue,html,css,scss,json,md}\"",
    "pre-commit": "husky install && lint-staged",
    "typecheck": "nuxt typecheck",
    "generate:api": "tsx scripts/codegen/api-client-generator.ts"
  },
  "dependencies": {
    "@auth/core": "^0.34.2",
    "@google/generative-ai": "^0.24.1",
    "@headlessui/vue": "^1.7.17",
    "@heroicons/vue": "^2.1.1",
    "@nuxt/eslint": "1.7.1",
    "@nuxt/image": "1.11.0",
    "@nuxt/scripts": "0.11.10",
    "@nuxt/test-utils": "3.19.2",
    "@nuxt/ui": "3.3.0",
    "@pinia/nuxt": "^0.5.1",
    "@prisma/client": "4.8.1",
    "@sendgrid/mail": "^8.1.5",
    "@simplewebauthn/browser": "^13.1.2",
    "@simplewebauthn/server": "^13.1.2",
    "@tailwindcss/vite": "^4.1.11",
    "@tiptap/pm": "^3.7.0",
    "@tiptap/starter-kit": "^3.7.0",
    "@tiptap/vue-3": "^3.7.0",
    "@types/cron": "^2.0.1",
    "@unhead/vue": "^2.0.3",
    "@vite-pwa/nuxt": "^1.0.4",
    "@vueuse/core": "^13.5.0",
    "aws-sdk": "^2.1012.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "core-js": "3.25.5",
    "cron": "^4.3.3",
    "date-fns": "^4.1.0",
    "eslint": "^9.0.0",
    "from": "^0.1.7",
    "import": "^0.0.6",
    "jsonwebtoken": "^9.0.2",
    "lucide-vue-next": "^0.525.0",
    "mock-aws-s3": "^4.0.2",
    "mongodb": "5.3.0",
    "motion-v": "^1.6.1",
    "nanoid": "4.0.2",
    "next-auth": "4.21.1",
    "nuxt": "^4.0.1",
    "openai": "^5.12.2",
    "pinia": "^2.1.7",
    "resend": "^6.0.2",
    "simplex-noise": "^4.0.3",
    "superjson": "^1.12.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tiktoken": "^1.0.22",
    "tw-animate-css": "^1.3.6",
    "vue": "^3.5.18",
    "dompurify": "^3.0.3",
    "vue-router": "^4.5.1",
    "web-push": "^3.6.7",
    "workbox-build": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0",
    "workbox-window": "^7.3.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.5.0",
    "@commitlint/config-conventional": "^18.5.0",
    "@nuxt/devtools": "^2.6.3",
    "@nuxt/schema": "^4.0.3",
    "@nuxt/types": "^2.18.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@playwright/test": "^1.55.0",
    "@sidebase/nuxt-auth": "^0.9.4",
    "@sidebase/nuxt-session": "^0.2.8",
    "@simplewebauthn/types": "^10.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^24.2.1",
    "@types/web-push": "^3.6.4",
    "commitizen": "^4.3.0",
    "esbuild": "^0.23.0",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "0.2.7",
    "prisma": "4.8.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vite-plugin-cross-origin-isolation": "^0.1.6",
    "vue-tsc": "^2.1.6"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.vue": [
      "eslint --fix",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
